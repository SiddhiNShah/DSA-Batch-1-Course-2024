class Solution {
public:
    int countStudents(vector<int>& students, vector<int>& sandwiches) {
        int count[2];
        for(int student : students)
        {
            count[student]++;
        }
        for(int i=0;i<sandwiches.size();i++)
        {
            if(count[sandwiches[i]]==0)
            return sandwiches.size()-i;
            count[sandwiches[i]]--;
        }
        return 0;
    }
};

Time Complexity : O(n)
Space Complexity : O(1)
